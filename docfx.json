{
  "metadata": [
    {
      "src": [
        {
          "files": [ "**/*.csproj" ],
          "exclude": [ "**/bin/**", "**/obj/**" ]
        }
      ],
      "dest": "api"
    }
  ],
  "build": {
    "content": [
      {
        "files": [ "api/**.yml", "api/index.md" ]
      },
      {
        "files": [ "articles/**.md", "index.md" ]
      }
    ],
    "dest": "_site",
    "globalMetadata": {
      "_appTitle": "Math.Lib Documentation"
    }
  }
}
